<%= form_tag(login_path) do %>

  <div class="form-control">
    <%= label_tag(:email) %>
    <%= email_field_tag(:email) %>
  </div>
  <div class="form-control">
    <%= label_tag(:password) %>
    <%= password_field_tag(:password) %>
  </div>

    <%= submit_tag("login", class: 'btn btn-info') %>
<% end %>